<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="createCustomer-Build-SOAP">
    <DisplayName>createCustomer Build SOAP</DisplayName>
    <Add>
        <Headers>
            <Header name="SOAPAction">http://www.easybill.de/webservice/customer/SetCustomer</Header>
        </Headers>
    </Add>
    <Set>
        <Headers>
            <Header name="Content-Type">text/xml; charset=utf-8</Header>
        </Headers>
        <Payload contentType="text/xml">
			<soap:Envelope 
          		xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/" 
          		xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
          		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          		xmlns:customer="http://www.easybill.de/webservice/customer/" 
				xmlns:common="http://www.easybill.de/webservice">
              	<soap:Header>
                  <common:UserAuthKey>{userkey}</common:UserAuthKey>
              	</soap:Header>
                <soap:Body>
                    <customer:SetCustomerRequest xmlns="http://www.easybill.de/webservice/" xmlns:SetCustomerRequest="http://www.easybill.de/webservice/">
                      	<!--Optional-->
                        <customerNumber>{customerNumber}</customerNumber>
                        <!--Required-->
                        <salutation>{salutation}</salutation>
                        <!--Optional-->
                        <personal>{personal}</personal>
                        <!--Optional-->
                        <title>{title}</title>
                        <!--Required-->
                        <firstName>{firstName}</firstName>
                        <!--Required-->
                        <lastName>{lastName}</lastName>
                        <!--Optional-->
                        <suffix_1>{suffix_1}</suffix_1>
                        <!--Optional-->
                        <suffix_2>{suffix_2}</suffix_2>
                        <!--Optional-->
                        <companyName>{companyName}</companyName>
                        <!--Required-->
                        <street>{street}</street>
                        <!--Required-->
                        <zipCode>{zipCode}</zipCode>
                        <!--Required-->
                        <city>{city}</city>
                        <!--Required-->
                        <country>{country}</country>
                        <!--Optional-->
                        <postbox>{postbox}</postbox>
                        <!--Optional-->
                        <postboxZipCode>{postboxZipCode}</postboxZipCode>
                        <!--Optional-->
                        <postboxCity>{postboxCity}</postboxCity>
                        <!--Optional-->
                        <postboxCountry>{postboxCountry}</postboxCountry>
                        <!--Optional-->
                        <phone_1>{phone_1}</phone_1>
                        <!--Optional-->
                        <phone_2>{phone_2}</phone_2>
                        <!--Optional-->
                        <fax>{fax}</fax>
                        <!--Optional-->
                        <mobile>{mobile}</mobile>
                        <!--Optional-->
                        <email>{email}</email>
                        <!--Optional-->
                        <internet>{internet}</internet>
                        <!--Optional-->
                        <info_1>{info_1}</info_1>
                        <!--Optional-->
                        <info_2>{info_2}</info_2>
                        <!--Optional-->
                        <note>{note}</note>
                        <!--Optional-->
                        <taxNumber>{taxNumber}</taxNumber>
                        <!--Optional-->
                        <ustid>{ustid}</ustid>
                        <!--Optional-->
                        <taxOptions>{taxOptions}</taxOptions>
                        <!--Optional-->
                        <cashAllowance>{cashAllowance}</cashAllowance>
                        <!--Optional-->
                        <cashDiscount>{cashDiscount}</cashDiscount>
                        <!--Optional-->
                        <cashDiscountType>{cashDiscountType}</cashDiscountType>
                        <!--Optional-->
                        <salePriceLevel>{salePriceLevel}</salePriceLevel>
                        <!--Optional-->
                        <bankAccountOwner_1>{bankAccountOwner_1}</bankAccountOwner_1>
                        <!--Optional-->
                        <bankName_1>{bankName_1}</bankName_1>
                        <!--Optional-->
                        <bankCode_1>{bankCode_1}</bankCode_1>
                        <!--Optional-->
                        <bankAccount_1>{bankAccount_1}</bankAccount_1>
                        <!--Optional-->
                        <bankBIC_1>{bankBIC_1}</bankBIC_1>
                        <!--Optional-->
                        <bankIBAN_1>{bankIBAN_1}</bankIBAN_1>
                        <!--Optional-->
                        <acquireOptions>{acquireOptions}</acquireOptions>
                        <!--Optional-->
                        <paymentOptions>{paymentOptions}</paymentOptions>
                        <!--Optional-->
                        <delivery_salutation>{delivery_salutation}</delivery_salutation>
                        <!--Optional-->
                        <delivery_personal>{delivery_personal}</delivery_personal>
                        <!--Optional-->
                        <delivery_firstName>{delivery_firstName}</delivery_firstName>
                        <!--Optional-->
                        <delivery_lastName>{delivery_lastName}</delivery_lastName>
                        <!--Optional-->
                        <delivery_suffix_1>{delivery_suffix_1}</delivery_suffix_1>
                        <!--Optional-->
                        <delivery_suffix_2>{delivery_suffix_2}</delivery_suffix_2>
                        <!--Optional-->
                        <delivery_companyName>{delivery_companyName}</delivery_companyName>
                        <!--Optional-->
                        <delivery_street>{delivery_street}</delivery_street>
                        <!--Optional-->
                        <delivery_zipCode>{delivery_zipCode}</delivery_zipCode>
                        <!--Optional-->
                        <delivery_city>{delivery_city}</delivery_city>
                        <!--Optional-->
                        <delivery_country>{delivery_country}</delivery_country>
                        <!--Optional-->
                        <birthDate>{birthDate}</birthDate>
                        <!--Optional-->
                        <sinceDate>{sinceDate}</sinceDate>
                        <!--Optional-->
                        <sepaAgreement>{sepaAgreement}</sepaAgreement>
                        <!--Optional-->
                        <sepaDate>{sepaDate}</sepaDate>
                        <!--Optional-->
                        <sepaInfo>{sepaInfo}</sepaInfo>
                    </customer:SetCustomerRequest>
                </soap:Body>
            </soap:Envelope>
        </Payload>
        <Verb>POST</Verb>
    </Set>
    <AssignVariable>
        <Name>forward.target.url</Name>
        <Value>http://soap2.easybill.de/webservices/webservice4/soap.easybill.php</Value>
    </AssignVariable>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
